#include <stdio.h>

int main() {
    int arr[5] = {10, 30, 40, 50, 60};
    int item = 20, i, pos = 2;

    printf("Given array elements are:\n");
    for (i = 0; i < 5; ++i) {
        printf("%d\t", arr[i]);
    }

    arr[pos - 1] = item;
    printf("\nArray elements after updation:\n");
    for (i = 0; i < 5; i++) {
        printf("arr[%d] = %d, ", i, arr[i]);
    }

    return 0;
}
